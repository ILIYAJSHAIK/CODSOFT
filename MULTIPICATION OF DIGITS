def print_multiples(number):
    print(f"Multiples {number} from 1 to 10")
    for i in range(1,11):
        print(f"{number} x {i} = {number * i}")
        
try:
    user_input = int(input("Enter the number : "))
    print_multiples(user_input)
except ValueError:
    print("Please enter a valid number.")
